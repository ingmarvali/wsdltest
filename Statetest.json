{
  "openapi": "3.0.1",
  "info": {
    "title": "StateOS Integration Guide",
    "description": "Services work using X-road protocol.Notification reciver is identified using personal code, phone number, or email provided in the URL.",
    "contact": {
      "email": "info@stateos.com"
    },
    "version": "2.6.0"
  },
  "servers": [
    {
      "url": "http://proxyserver:8089/cgi-bin/consumer_proxy"
    }
  ],
  "tags": [
    {
      "name": "Permission API",
      "description": "API integration for checking permission status."
    },
    {
      "name": "Message API",
      "description": "API integration for sending messages."
    }
  ],
  "paths": {
    "/provider/permission/{messageType}": {
      "post": {
        "tags": [
          "Permission API"
        ],
        "summary": "Get permission status for list of StateOS users",
        "description": "Service returns a list of StateOS user contacts the provider has permission to send that type of messages. Input for the service is a list of phone numbers, e-mails or personal codes.New providers will be added to users within 24 hours after adding the provider to the system. For end users it is an Opt-Out solution where all providers are allowed on registration, but users can manage permissions for providers based on message type",
        "operationId": "getConsentAcceptedUsersForProviderByPersonalCode",
        "parameters": [
          {
            "in": "header",
            "name": "X-Road-Client",
            "example": "ee-test/GOV/70000310/eregistrations",
            "description": "Format is: INSTANCE/CLASS/MEMBER/SUBSYSTEM",
            "schema": {
              "type": "string"
            },
            "required": true
          },
          {
            "name": "messageType",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "ADVERTISEMENT",
                "NOTIFICATION"
              ]
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "title": "Contact List Permission Request",
                "type": "object",
                "properties": {
                  "contactList": {
                    "maxItems": 5000,
                    "minItems": 1,
                    "type": "array",
                    "items": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "title": "Contact List Permission Response",
                  "type": "object",
                  "properties": {
                    "allowedUsers": {
                      "uniqueItems": true,
                      "type": "array",
                      "items": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "*/*": {
                "schema": {
                  "title": "Bad Request Exception Message",
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "enum": [
                        "BAD_REQUEST",
                        "CONTENT_MISSING",
                        "CONTENT_TOO_LONG",
                        "CONTENT_URL_NOT_VALID"
                      ]
                    }
                  }
                }
              }
            }
          },
          "403": {
            "description": "Forbidden",
            "content": {
              "*/*": {
                "schema": {
                  "title": "Forbidden Exception Message",
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "enum": [
                        "FORBIDDEN"
                      ]
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "*/*": {
                "schema": {
                  "title": "Not Found Exception Message",
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "enum": [
                        "USER_NOT_FOUND",
                        "PROVIDER_NOT_FOUND",
                        "XROAD_INSTANCE_NOT_FOUND",
                        "CONSENT_NOT_FOUND",
                        "CONSENT_ACTION_NOT_FOUND"
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/provider/message/{messageType}": {
      "post": {
        "tags": [
          "Message API"
        ],
        "summary": "Send mass message to all StateOS users",
        "description": "A message will be sent to all StateOS users who have not removed the permissions from the provider to send messages of that type",
        "operationId": "createMassNotification",
        "parameters": [
          {
            "in": "header",
            "name": "X-Road-Client",
            "example": "ee-test/GOV/70000310/eregistrations",
            "description": "Format is: INSTANCE/CLASS/MEMBER/SUBSYSTEM",
            "schema": {
              "type": "string"
            },
            "required": true
          },
          {
            "name": "messageType",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "ADVERTISEMENT",
                "NOTIFICATION"
              ]
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "title": "Message Request",
                "required": [
                  "content"
                ],
                "type": "object",
                "properties": {
                  "image": {
                    "type": "array",
                    "items": {
                      "type": "string",
                      "format": "byte"
                    }
                  },
                  "contentUrl": {
                    "type": "string"
                  },
                  "content": {
                    "maxLength": 240,
                    "minLength": 1,
                    "type": "string"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "title": "Mass Message Response",
                  "type": "object",
                  "properties": {
                    "successfulCount": {
                      "type": "integer",
                      "format": "int32"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "*/*": {
                "schema": {
                  "title": "Bad Request Exception Message",
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "enum": [
                        "BAD_REQUEST",
                        "CONTENT_MISSING",
                        "CONTENT_TOO_LONG",
                        "CONTENT_URL_NOT_VALID"
                      ]
                    }
                  }
                }
              }
            }
          },
          "403": {
            "description": "Forbidden",
            "content": {
              "*/*": {
                "schema": {
                  "title": "Forbidden Exception Message",
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "enum": [
                        "FORBIDDEN"
                      ]
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Not Found",
            "content": {
              "*/*": {
                "schema": {
                  "title": "Not Found Exception Message",
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "enum": [
                        "USER_NOT_FOUND",
                        "PROVIDER_NOT_FOUND",
                        "XROAD_INSTANCE_NOT_FOUND",
                        "CONSENT_NOT_FOUND",
                        "CONSENT_ACTION_NOT_FOUND"
                      ]
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
